{
  "name": "should-i-use-effect",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "01": "npm run dev --workspace=apps/01",
    "02": "npm run dev --workspace=apps/02",
    "03": "npm run dev --workspace=apps/03",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test:01": "npm test --workspace=apps/01",
    "test:02": "npm test --workspace=apps/02",
    "test:03": "npm test --workspace=apps/03"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aronmartin/should-i-use-effect.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aronmartin/should-i-use-effect/issues"
  },
  "homepage": "https://github.com/aronmartin/should-i-use-effect#readme",
  "description": "A monorepo with three React Vite apps and a shared UI library",
  "devDependencies": {
    "@types/mocha": "^10.0.10"
  }
}
